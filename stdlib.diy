;; Some logical operators.

(define not
    (lambda (b)
        (if b #f #t)))

;; DIY -- Implement the rest of your standard library
;; here as part 7 of the workshop.

(define or
  (lambda (l r)
    (if l #t
      (if r #t #f))))

(define and
  (lambda (l r)
    (if l
      (if r #t #f)
      #f)))

(define xor
  (lambda (l r)
    (if l
      (if r #f #t)
      (if r #t #f))))

(define >=
  (lambda (l r)
    (if (> l r)
      #t
      (if (> r l) #f #t))))
